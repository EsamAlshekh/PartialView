@using PartialView.Models

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>

    @foreach (var item in PerCar.People)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
        </tr>
    }
    
</table>
<br /><br />
<table>

    @foreach (var item in PerCar.Cars)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Brand</td>
            <td>@item.Name</td>
        </tr>
    }
</table>

